{
  "name": "phone-prettify",
  "version": "1.3.0",
  "description": "Simple phone formatting",
  "main": "dist/phone-prettify.umd.js",
  "scripts": {
    "prepublish": "rollup -c",
    "rollup:tests": "rollup -i tests/tests.js -f cjs -o tests/tests.cjs.js -e tape,moment && rollup -c",
    "pretest": "npm run rollup:tests",
    "test": "tape tests/tests.cjs.js | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhershman1/phone-prettify"
  },
  "bugs": {
    "url": "https://github.com/dhershman1/phone-prettify/issues"
  },
  "keywords": [
    "format",
    "phone number",
    "prettyify",
    "phone",
    "phone-prettify"
  ],
  "author": "Dustin Hershman <dustinh17@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-uglify": "^2.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  }
}
